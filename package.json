  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adrianlivr/expo-svg-shadow"
  },
  "author": "Adriano Oliveira",
  "license": "MIT",
  "dependencies": {
    "react-native-svg": "12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.63.51",
    "eslint": "^7.21.0",
    "eslint-config-react-native-typescript": "^2.2.6",
    "jest": "^26.6.3",
    "jest-expo": "^40.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-builder-bob": "^0.18.1",
    "react-native-testing-library": "^6.0.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
